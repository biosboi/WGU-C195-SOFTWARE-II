<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="430.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.customerMenuController">
   <children>
      <TableView fx:id="customerTable" layoutX="13.0" layoutY="49.0" prefHeight="298.0" prefWidth="670.0">
        <columns>
          <TableColumn fx:id="customerTable_ID" editable="false" prefWidth="39.333312034606934" text="ID" />
          <TableColumn fx:id="customerTable_Name" prefWidth="147.33337020874023" text="Name" />
            <TableColumn fx:id="customerTable_Address" prefWidth="134.6667022705078" text="Address" />
            <TableColumn fx:id="customerTable_PostalCode" minWidth="0.0" prefWidth="71.33331298828125" text="Postal Code" />
            <TableColumn fx:id="customerTable_Phone" minWidth="4.33331298828125" prefWidth="103.6666259765625" text="Phone" />
            <TableColumn fx:id="customerTable_comboBox" prefWidth="170">
               <graphic>
                  <ComboBox fx:id="customerTable_comboBox_switch" onAction="#switchComboBox" prefHeight="25.0" prefWidth="170.0" promptText="Division ID" visibleRowCount="2" />
               </graphic></TableColumn>
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="23.0" text="Customer View">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Button fx:id="returnButton" layoutX="534.0" layoutY="396.0" mnemonicParsing="false" onAction="#previousMenu" text="Return" />
      <Button fx:id="removeButton" layoutX="60.0" layoutY="362.0" mnemonicParsing="false" onAction="#removeCustomer" text="Remove" />
      <Button fx:id="addButton" layoutX="14.0" layoutY="362.0" mnemonicParsing="false" onAction="#addCustomer" text="Add" />
   </children>
</AnchorPane>
