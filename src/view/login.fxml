      <Label fx:id="passwordLabel" layoutX="29.0" layoutY="133.0" text="Password" />
      <Label fx:id="locationLabel" layoutX="31.0" layoutY="213.0" text="Location" />
      <Button fx:id="loginButton" layoutX="148.0" layoutY="179.0" mnemonicParsing="false" onAction="#loginButton_click" text="Login" />
